{
  "name": "logux-ngrx",
  "version": "0.0.0",
  "description": "NgRx compatible API for Logux",
  "keywords": [
    "logux",
    "client",
    "angular",
    "ngrx"
  ],
  "scripts": {
    "build": "ng build",
    "test": "jest-ci --coverage && eslint-ci . && check-dts && size-limit && yarn spell",
    "spell": "yaspeller-ci *.md",
    "clean": "rimraf coverage/"
  },
  "author": "Nikolay Govorov <nikolay_govorov@bk.ru>",
  "license": "MIT",
  "homepage": "https://logux.io/",
  "repository": "nikolay-govorov/logux-ngrx",
  "sideEffects": false,
  "peerDependencies": {
    "@angular/common": "^9.1.1",
    "@angular/core": "^9.1.1",
    "tslib": "^1.10.0",
    "@ngrx/store": "^9.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular-devkit/build-ng-packagr": "~0.901.1",
    "@angular/animations": "~9.1.1",
    "@angular/cli": "~9.1.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@logux/client": "^0.8.4",
    "@logux/eslint-config": "^36.1.1",
    "@logux/sharec-config": "^0.6.2",
    "@ngrx/store": "^9.1.0",
    "@size-limit/dual-publish": "^4.3.1",
    "@size-limit/preset-small-lib": "^4.3.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "check-dts": "^0.1.3",
    "codelyzer": "^5.1.2",
    "dual-publish": "^0.5.1",
    "eslint": "^6.8.0",
    "eslint-ci": "^1.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^17.2.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^25.1.0",
    "jest-ci": "^0.1.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.0.8",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "rimraf": "^3.0.2",
    "rxjs": "~6.5.4",
    "size-limit": "^4.3.1",
    "ts-node": "~8.3.0",
    "tslib": "^1.10.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "yaspeller-ci": "^1.0.2",
    "zone.js": "~0.10.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller",
    "*.js": "eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ]
  },
  "sharec": {
    "config": "@logux/sharec-config",
    "version": "0.6.2"
  }
}
